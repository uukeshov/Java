package Chapter2;

public class Number38 {
	public static int a = 212335, counter, len, newa, i = 0, k;
	public static String b;

	public static void main(String[] args) {
		b = Integer.toString(a);
		len = b.length();
		System.out.println(b);

		while (i < len) {
			k = b.charAt(len);

			if (k == 1) {
				b = b.substring(0, len);
			}
			i++;
		}
//System.out.println(b);
	}

}
