package Chapter5;

import java.util.*;

public class Number8 {

	public static void main(String[] args) {
		int[] ceil_numbers = { 2, 3, 11 };
		String simpleNumbers = "";
		int i = 0, j, temp, ceil = 0, control;
		i = 0;
		while (i < ceil_numbers.length) {
			temp = ceil_numbers[i];
			ceil = 0;
			j = 2;
			while (j <= temp) {
				if (temp != j) {
					ceil = temp % j;
					if (ceil == 0) {
						control = 1;
					}
				}

				j++;
			}
			if (control == 0 && temp != 1) {
				simpleNumbers = simpleNumbers + " " + String.valueOf(temp);
			}

			i++;
		}
		System.out.println(simpleNumbers);
	}
}
