package Chapter1;

public class Number5 {
public static int a=21,c;
public static String b,k;
	public static void main(String[] args) {
		
		b=Integer.toString(a);
		c=Integer.valueOf(b.substring(0,0));
				//+Integer.valueOf(b.substring(1,2)));
		System.out.println(c);
		c=(int)Math.pow(c, 2);
		System.out.println(c);
		
		

	}

}
