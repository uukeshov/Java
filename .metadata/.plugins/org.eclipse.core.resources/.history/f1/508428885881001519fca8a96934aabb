package Chapter1;

public class Number5 {
	public static int a = 21, c, e;
	public static String b, k;

	public static void main(String[] args) {

		b = Integer.toString(a);
		c = Integer.valueOf(b.substring(0, 1));
		e = Integer.valueOf(b.substring(1, 2));
		System.out.println(c);
		c = (int) Math.pow(c+e, 2);
		System.out.println(c);

	}

}
