package Chapter2;

public class Number24 {

	public static void main(String[] args) {
		int first = 1, second = 1;

		for (int i = 0; i < 50; i++) {

			if (first == 1 && second == 1) {
				System.out.print(first);
				System.out.print(second);
				second = first + second;
				first = second;
			} else {
				System.out.print(second + first);
				second = first + second;
				first = second;
			}

		}
	}
}
