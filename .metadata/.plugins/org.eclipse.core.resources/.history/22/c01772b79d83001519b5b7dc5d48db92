package Chapter6;

public class Number8 {
	public static int[] array1 = { 2, 3, 4, 5, 33, 22, 11 }, array2 = { 2, 3, 4, 5, 33, 22, 11 };
	public static int len, result=0,temp=0;

	public static void main(String[] args) {
		
		for (int i=0;i<=array1.length;i++) {
			temp=array1[i]*array2[i];
			result=result+temp;
		}
		System.out.println(result);

	}

}
